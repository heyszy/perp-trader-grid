# 网格参数
GRID_STRATEGY_ID=grid-default
GRID_SYMBOL=BTC
GRID_LEVELS=10
GRID_SPACING_MODE=ABS
GRID_SPACING=10
GRID_SPACING_PERCENT=0.002
GRID_QUANTITY=0.001
GRID_POST_ONLY=true
GRID_CANCEL_TIMEOUT_MS=60000
GRID_MAX_POSITION=0.01
GRID_MAX_OPEN_ORDERS=40

# 调试日志
DEBUG_MARKET_LOG=false

# 数据库
DB_PATH=./data/perp-grid.db

# 交易所选择（extended | nado | hyperliquid）
EXCHANGE=extended

# Extended 账户与网络
EXTENDED_API_KEY=
EXTENDED_L2_PRIVATE_KEY=
EXTENDED_NETWORK=mainnet

# Nado 账户与网络（inkMainnet）
NADO_PRIVATE_KEY=
NADO_RPC_URL=https://rpc-gel.inkonchain.com
NADO_SUBACCOUNT_NAMES=default

# Hyperliquid 账户与网络
HYPERLIQUID_PRIVATE_KEY=
# 账户地址（主账户或子账户，使用 agent key 时必须填写）
HYPERLIQUID_USER_ADDRESS=
HYPERLIQUID_NETWORK=mainnet
HYPERLIQUID_DEX=
# 设置 HYPERLIQUID_DEX 时，GRID_SYMBOL=XYZ100 会自动映射为 dex:XYZ100
HYPERLIQUID_MIN_NOTIONAL=10

# 通知（可选）
BARK_SERVER=
# 逗号分隔多个 key，例如 key1,key2
BARK_KEYS=
